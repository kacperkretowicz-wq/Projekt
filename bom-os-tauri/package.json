
{
  "name": "bom-os-tauri",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "tauri:dev": "concurrently -k "npm:dev" "npm:flask:dev" "npm:tauri-only:dev"",
    "tauri-only:dev": "wait-on http://localhost:5173 && tauri dev",
    "flask:dev": "python pyserver/app.py",
    "tauri:build": "vite build && tauri build"
  },
  "devDependencies": {
    "@tauri-apps/cli": "^2.0.0",
    "@types/node": "^20.0.0",
    "@types/react": "^18.2.70",
    "@types/react-dom": "^18.2.24",
    "concurrently": "^8.2.2",
    "typescript": "^5.5.0",
    "vite": "^5.2.0",
    "wait-on": "^7.2.0"
  },
  "dependencies": {
    "@tauri-apps/api": "^2.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  }
}
